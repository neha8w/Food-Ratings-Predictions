nto a non-existent directory: '{parent}'\"\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m    617\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m    618\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n","\u001b[0;31mOSError\u001b[0m: Cannot save file into a non-existent directory: '/mnt/data'"]}],"source":["import pandas as pd\n","from sklearn.model_selection import train_test_split\n","from sklearn.ensemble import RandomForestRegressor\n","from sklearn.metrics import mean_absolute_error\n","from sklearn.preprocessing import LabelEncoder\n","from sklearn.feature_extraction.text import TfidfVectorizer\n","from textblob import TextBlob\n","\n","# Load train and test datasets\n","train_data = pd.read_csv(\"/content/train.csv\")\n","test_data = pd.read_csv(\"/content/test.csv\")\n","\n","# Data Preprocessing\n","# Handle missing target values in train dataset\n","train_data = train_data[train_data['Rating'] \u003e 0]\n","\n","# Feature Engineering\n","# Text feature extraction using TF-IDF\n","vectorizer = TfidfVectorizer(stop_words='english', max_features=500)\n","X_text_train = vectorizer.fit_transform(train_data['Recipe_Review'].fillna(\"\"))\n","X_text_test = vectorizer.transform(test_data['Recipe_Review'].fillna(\"\"))\n","\n","# Encoding categorical features\n","label_encoder = LabelEncoder()\n","train_data['RecipeName_encoded'] = label_encoder.fit_transform(train_data['RecipeName'])\n","test_data['RecipeName_encoded'] = label_encoder.transform(test_data['RecipeName'])\n","\n","# Combine numerical and text features for train and test datasets\n","X_train = pd.concat([\n","    train_data[['UserReputation', 'ReplyCount', 'ThumbsUpCount', 'ThumbsDownCount', 'RecipeNumber', 'RecipeName_encoded']],\n","    pd.DataFrame(X_text_train.toarray(), index=train_data.index)\n","], axis=1)\n","\n","X_test = pd.concat([\n","    test_data[['UserReputation', 'ReplyCount', 'ThumbsUpCount', 'ThumbsDownCount', 'RecipeNumber', 'RecipeName_encoded']],\n","    pd.DataFrame(X_text_test.toarray(), index=test_data.index)\n","], axis=1)\n","\n","X_train.columns = X_train.columns.astype(str)\n","X_test.columns = X_test.columns.astype(str)\n","\n","\n","# Target variable\n","y_train = train_data['Rating']\n","\n","# Train-Test Split for Validation\n","X_train_split, X_val_split, y_train_split, y_val_split = train_test_split(X_train, y_train, test_size=0.2, random_state=42)\n","\n","# Model Training\n","model = RandomForestRegressor(n_estimators=100, random_state=42)\n","model.fit(X_train_split, y_train_split)\n","\n","# Validation Prediction and Evaluation\n","y_val_pred = model.predict(X_val_split)\n","mae = mean_absolute_error(y_val_split, y_val_pred)\n","print(f\"Validation Mean Absolute Error: {mae:.2f}\")\n","\n","# Final Prediction on Test Set\n","y_test_pred = model.predict(X_test)\n","\n","# Save Predictions\n","output = test_data[['CommentID']]\n","output['PredictedRating'] = y_test_pred\n","output.to_csv('/mnt/data/test_predictions.csv', index=False)\n","\n","print(\"Predictions saved to 'test_predictions.csv'.\")\n"]}],"metadata":{"colab":{"authorship_tag":"ABX9TyPPvPpkv/B2KcgVa1gT9sKr","name":"","version":""},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}